<Window x:Class="GMS_LotteryTracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GMS_LotteryTracker"
        mc:Ignorable="d"
        Title="GMS_LotteryTracker" Height="450" Width="600" ResizeMode="CanMinimize">
    <Grid>
        <TabControl Name="MainControl" ItemsSource="{Binding}" SelectionChanged="TabItem_MouseLeftButtonDown"/>
    </Grid>
</Window>
